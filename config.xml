<?xml version="1.0" encoding="UTF-8" ?>
<widget xmlns   = "http://www.w3.org/ns/widgets"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:gap   = "http://phonegap.com/ns/1.0"
    id          = "com.phonegap.example"
    versionCode = "10"
    version     = "1.0.0" >


    <platform name="android">
        <config-file parent="/*" target="AndroidManifest.xml">
          <uses-permission android:name="android.permission.CAMERA"/>
          <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
          <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
        </config-file>
    </platform>

<!-- versionCode is optional and Android only -->

  <name>Cociname</name>
  <description>
      Cociname es un proyecto de fin de curso
  </description>

  <author href="https://build.phonegap.com" email="mattiaa95@gmail.com">
      Mattiaa95
  </author>

<plugin name="cordova-plugin-whitelist" spec="1" />


  <!-- Allow images, xhrs, etc. to google.com -->
<access origin="http://10.10.100.13" />

<!-- Enable requests to content: URLs -->
<access origin="content:///*" />

<!-- Don't block any requests -->
<access origin="*" />


<!-- Allow links to example.com -->
<allow-navigation href="http://10.10.100.13/*" />

<!-- A wildcard can be used to whitelist the entire network,
     over HTTP and HTTPS.
     *NOT RECOMMENDED* -->
<allow-navigation href="*" />
<allow-navigation href="data:*" />


<!-- The above is equivalent to these three declarations -->
<allow-navigation href="http://*/*" />
<allow-navigation href="data:*" />

<!-- Allow links to web pages to open in a browser -->
<allow-intent href="http://*/*" />
<allow-intent href="https://*/*" />

<!-- Allow links to example.com to open in a browser -->
<allow-intent href="http://10.10.100.13/*" />

<!-- Allow SMS links to open messaging app -->
<allow-intent href="sms:*" />

<!-- Allow tel: links to open the dialer -->
<allow-intent href="tel:*" />

<!-- Allow geo: links to open maps -->
<allow-intent href="geo:*" />

<!-- Allow all unrecognized URLs to open installed apps
     *NOT RECOMMENDED* -->
<allow-intent href="*" />
</widget>
